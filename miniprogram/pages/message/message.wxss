page{
  height:100%;
  width: 100%;
  overflow: auto;
  background: rgb(16, 18, 30);
}
.all-wrap{
  height: 100%;
  width: 100%;
}
.ava-head-wrap{
  width: 100%;
  height: 130px;
  background: rgb(85, 215, 207);
  display: flex;
  justify-content: center;
  align-content: center;
  flex-wrap: wrap;
}
.ava-head-border{
  height: 65px;
  width: 65px;
  border: 3px solid #99e7ec;
  border-radius: 50%;
  overflow: hidden;
}
.ava-head-pic{
  height: 65px;
  width: 65px;
  border-radius: 50%;
}
.ava-head-text{
  color:white;
  font-size:14px;
  width: 100%;
  text-align: center;
  margin-top: 4px;
}
.content-wrap{
  width:100%;
  height: calc(100% - 130px);
  background: rgb(16, 18, 30);
  padding-top:15px;
  display: flex;
  justify-content: center;
  /* align-content: center; */
  flex-wrap: wrap;
}
.content-mes{
  color:rgb(185, 241, 238);
  width: 90%;
  border: 1px solid #679a95;
  text-align: center;
  height: 36px;
  border-radius: 10px;
  line-height: 36px;
}
.input-wrap{
  width:90%;
  height: 45px;
  border: 1px solid #679a95;
  display: flex;
  border-radius:10px;
  justify-content: space-around;
}
.chink{
  width:92%;
  height: 41px;
}
.button-wrap{
  width:90%;
  height: 60px;
  display: flex;
}
.input-label{
  color:white;
  height: 100%;
  line-height: 45px;
  width:90px;
  text-align: left;
  font-size: 13px;
  padding-left: 38px;
}
.input-input{
  /* width: 0px;
  height: 0px; */
  width: calc(100% - 120px);
  height: 45px;
  color: white;
  line-height: 45px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.input-content{
  width: calc(100% - 120px);
  height: 45px;
  color: white;
  line-height: 45px;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.button-css{
  background: rgb(85, 215, 207);
  color: white;
  width: 120px;
  height: 44px;
  border-radius: 23px;
  font-size: 14px; 
  line-height: 44px;
}

.popup-wrap{
  width:280px;
  height:200px;
  display: flex;
  flex-wrap: wrap;
}
.popup-wrap text{
  width:100%;
  height:30px;
  font-size: 13px;
  color:rgb(93, 93, 93);
  padding-left: 10px;
}
.popup-title{
  font-weight: bold;
  text-align: center;
  padding-left:0px !important;
  font-size: 16px !important;
}
.popup-btn-wrap{
   width:100%;
   border-top:1px solid #cccccc;
}
.popup-btn{
  color:rgb(85, 215, 207);
}
.tips{
  width:90%;
  height:auto;
  display: flex;
  flex-wrap: wrap;
  padding: 0 5px 0 5px;
}
.tips > text{
  font-size: 14px;
  width: 100%;
  color: white;
}
.tips-head{
  font-size: 16px !important;
}
.vxCss{
  color: rgb(85, 215, 207) !important;
  font-weight: bold;
}

